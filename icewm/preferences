# Show workspace switching buttons on task bar
TaskBarShowWorkspaces=1 # 0 / 1

# Place workspace buttons on the left, instead of the right
TaskBarWorkspacesLeft=1 # 0 / 1

# Names of the Workspaces
WorkspaceNames="1","2","3","4"

# Color of normal workspace button text
ColorNormalWorkspaceButtonText="rgb:00/00/00"

# Color of active workspace button text
ColorActiveWorkspaceButtonText="rgb:00/00/EE"

# Color of normal workspace button
ColorNormalWorkspaceButton="rgb:C0/C0/C0"

# Color of active workspace button
ColorActiveWorkspaceButton="rgb:A0/A0/A0"

# Normal workspace  font
NormalWorkspaceFontName="-liberation-mono-medium-r-*-*-*-120-*-*-*-*-*-*"
NormalWorkspaceFontNameXft="liberation-mono:size=11:medium"

# Active workspace  font
ActiveWorkspaceFontName="-liberation-mono-bold-r-*-*-*-120-*-*-*-*-*-*"
ActiveWorkspaceFontNameXft="liberation-mono:size=11:bold"

# Show workspace tooltip when changing workspaces
ShowWorkspaceStatus=1 # 0 / 1

# Status font
StatusFontName="-liberation-mono-bold-r-*-*-*-120-*-*-*-*-*-*"
StatusFontNameXft="liberation-mono:size=11:bold"

# Number of milliseconds to display the workspace tooltip when changing workspaces
WorkspaceStatusTime=2500 # 0-6000

# Screen edge workspace switching delay
#EdgeSwitchDelay=600 # 0-5000

#  Show application icons in the tray panel
TaskBarShowTray=0 # 0/1

#  Enable the system tray in the taskbar
TaskBarEnableSystemTray=0 # 0/1

#  Show Ellipsis in taskbar items
ShowEllipsis=0 # 0/1

#  Show APM/ACPI/Battery/Power status monitor on task bar
TaskBarShowAPMStatus=0 # 0/1

#  Enable TaskBarShowAPMStatus if a battery is present
TaskBarShowAPMAuto=0 # 0/1

#  Show APM status on task bar in time-format
TaskBarShowAPMTime=0 # 0/1

#  Show memory usage status on task bar (Linux only)
TaskBarShowMEMStatus=1 # 0/1

#Hide Focus Mode thing
ShowFocusModeMenu=0
